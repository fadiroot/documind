{
  "include": [
    "app",
    "core",
    "agents",
    "scripts",
    "tests"
  ],
  "exclude": [
    "**/__pycache__",
    ".venv",
    "venv"
  ],
  "pythonVersion": "3.11",
  "pythonPlatform": "All",
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "venvPath": ".",
  "venv": ".venv"
}
